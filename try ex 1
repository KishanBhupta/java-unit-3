
class tryblock
{
    public static void main(String args[]){
        try{
            int a[] = new int [5];
            a[5] =4;
                     
        }
        catch(ArrayIndexOutOfBoundsException e)
        {
            System.out.println(e);
        }
        catch(Exception e)
    {
        System.out.println("Exception handled");
    }
    System.out.println("Casual Flow");
    }
}
